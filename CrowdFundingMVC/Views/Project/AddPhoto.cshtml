@using CrowdFundingMVC.Models.ProjectController
@model AddPhotoViewModel

<form asp-action="AddPhoto" id='formID' enctype="multipart/form-data">
    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-group" style="color:silver;">
        <input asp-for="@Model.Photo"  type="file" id="Photo" name="Photo">
        <input type="hidden" asp-for="@Model.ProjectId" />
    </div>

    <button type="submit"  id='submitID' class="btn btn-primary mt-3">Submit</button>
</form>